	<script>
	function getUrlVars() {
	    //県番号の数字の先頭位置をとる
	    var pos = window.location.href.indexOf("index/") + 6;
	    //県番号の数字以降を切り出す
	    var kenstr = window.location.href.substr(pos);
	    //数字以降の文字を10進数に変換する
	    return parseInt(kenstr,10);
	}
	$(document).ready(function(){
	    var ken =  getUrlVars();
	    //クッキー保存　999日間有効
	    $.cookie('ken', ken, {expires: 999, path: '/'});
	});

	$(function() {
		// # ハッシュURLによりタブを変更
		var tabCat = (location.hash.match(/^#!\/tab\/([a-z0-9]+)/i) || [])[1];
		if (tabCat) {
			$('#tab-category li').each(function (key) {
				if (this.id == 'tab-'+tabCat) {
					$(this).addClass('is_active');
					$('#box-category .c-tab__cont-item').eq(key).addClass('is_show');
				} else {
					$(this).removeClass('is_active');
					$('#box-category .c-tab__cont-item').eq(key).removeClass('is_show');
				}
			});
		}
	});
	</script>
